* fanstastically fanstasic

  package main
  
  import "fmt"
  
  func f(a int, b int) {
  	var min = 0
  	fmt.Sscanf(fmt.Sprintf("%d %t %d", a, a > b, b), "%d true %d", &min, &min)
  	fmt.Printf("The min of %d and %d is %d\n", a, b, min)
  }
  
  func main() {
  	f(9000, 314)
  }

* creative

  package main
  
  import "fmt"
  
  func f(a int, b int) {
  	var min = 0
  	min = len(fmt.Sprintf("%.[1]*s", a, fmt.Sprintf("%[1]*s", b, "_")))
  
  	fmt.Printf("The min of %d and %d is %d\n", a, b, min)
  }
  
  func main() {
  	f(9000, 3314)
  }

